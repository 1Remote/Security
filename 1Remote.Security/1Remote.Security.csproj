<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>net6.0;netcoreapp3.1;net48;</TargetFrameworks>
		<AssemblyName>1Remote.Security</AssemblyName>
		<RootNamespace>_1Remote.Security</RootNamespace>
		<Nullable>enable</Nullable>
		<LangVersion>latest</LangVersion>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<PackageProjectUrl>https://github.com/1Remote</PackageProjectUrl>
		<Authors>VShawn</Authors>
		<Company>VShawn</Company>
		<PackageIcon>logo256.png</PackageIcon>
	</PropertyGroup>

	<ItemGroup>
	  <None Include="..\doc\icons\logo256.png">
	    <Pack>True</Pack>
	    <PackagePath>\</PackagePath>
	  </None>
	</ItemGroup>

</Project>
